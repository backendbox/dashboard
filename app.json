{
  "name": "Camille Baldock Life Dashboard",
  "description": "A dashing app with various monitoring widgets",
  "repository": "https://github.com/camilleldn/life-dashboard",
  "keywords": ["dashing", "ruby"],
  "env": {
    "TRELLO_DEVELOPER_KEY": "trello_developer_key",
    "TRELLO_MEMBER_TOKEN": "trello_member_token",
    "TRELLO_BOARD_NAME" : "trello_board_id",
    "TRELLO_LIST_NAMES": "name,othername",
    "GITHUB_TOKEN": "github_oauth_token",
    "GITHUB_USER": "camilleldn",
    "AUTH_TOKEN": "auth_token",
    "GEMNASIUM_TOKEN": "gemnasium_token",
    "UPDATE_FREQUENCY": "10m",
    "BOXEN_REPO": "camilleldn/boxen",
    "STATUSCAKE_USERNAME":"statuscake_username",
    "STATUSCAKE_KEY":"statuscake_key",
    "GMAIL_USER":"you@gmail.com",
    "GMAIL_PASSWORD":"gmail-password",
    "RESCUETIME_KEY": "rescuetime_key",
    "GOODREADS_LIST_ID": "goodreads_list_id",
    "GOODREADS_KEY": "goodreads_key",
    "FEEDLY_TOKEN": "feedly_token",
    "AMAZON_EMAIL": "amazon_email",
    "AMAZON_PASSWORD": "amazon_password",
    "WUNDERLIST_CLIENT_TOKEN": "wunderlist_token",
    "WUNDERLIST_CLIENT_ID": "wunderlist_id",
    "POCKET_CONSUMER_KEY": "consumer_key",
    "POCKET_ACCESS_TOKEN": "access_token",
    "POCKET_CASTS_EMAIL": "pocketcasts_email",
    "POCKET_CASTS_PASSWORD": "pocketcasts_password",
    "TRAVIS_REPOS": "comma,separated,list,of,your,repo,names",
    "PROJECT_EULER_USERNAME": "project_euler_username",
    "PROJECT_EULER_PASSWORD": "project_euler_password",
    "MANUAL_FORKS":"user/parent,otheruser/fork;user2/parent,otheruser2/fork",
    "MONITORED_GITHUB_ORGS":"names,of,github,orgs,to,monitor,for,pulls,and,issues",
    "ANCESTRY_EMAIL":"ancestry_email",
    "ANCESTRY_PASSWORD":"ancestry_password"
  },
  "addons": [
    "redistogo:nano"
  ]
}
